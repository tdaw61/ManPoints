<% if params[:modal] == "false" %>
$(".status-content").html("<%= escape_javascript(render 'vote_form')%>");
$("#event-tab").attr("class", "active");
$("#post-tab").attr("class", "inactive");
<% else %>
$("#vote-window").find(".modal-content").html("<%= escape_javascript(render 'games/modal_vote_form') %>")
<% end %>